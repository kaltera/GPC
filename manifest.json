{
  "manifest_version": 2,

  "name": "Gartner Post Call",
  "description": "Get Document IDs for each tab",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches" : ["*://*.gartner.com/*"],
      "js": ["dom.js"]
    }
  ],

  "permissions": [
    "tabs",
    "activeTab",
    "clipboardWrite",
    "clipboardRead",
    "https://ajax.googleapis.com/"
  ]
}