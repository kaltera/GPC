{
  "manifest_version": 2,

  "name": "GPC",
  "description": "Get Document IDs for each tab",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "activeTab",
    "clipboardWrite",
    "clipboardRead",
    "https://ajax.googleapis.com/"
  ]
}